  <!--根据主键id批量删除-->
  <delete id="deleteByIds">
    delete from sys_user where id in
    <foreach item="id" collection="ids" open="(" separator="," close=")">
      #{id}
    </foreach>
  </delete>

  <select id="findUserByAccountName" resultMap="BaseResultMap">
     SELECT
     id,account_name,pass_word,NAME,CODE,sex,birthday,entry_date,
     department_id,photo_id,phone,email,telephone,contacts,contacts_phone,address,school,major,graduation,
